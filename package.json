{
  "name": "deepresearch",
  "version": "1.0.0",
  "description": "Node.js backend clone of the Python LangGraph research agent",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --watch src -e ts,json,ejs --exec \"npm run build && node dist/app.js\"",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "research",
    "agent",
    "langgraph",
    "ai",
    "backend"
  ],
  "author": "DeepResearch Team",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@langchain/core": "^0.2.0",
    "@langchain/google-genai": "^0.0.25",
    "axios": "^1.6.0",
    "compression": "^1.8.1",
    "console-log-colors": "^0.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^155.0.1",
    "helmet": "^7.2.0",
    "http-errors": "^2.0.0",
    "joi": "^17.11.0",
    "langchain": "^0.2.0",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^2.20.33",
    "@types/http-errors": "^2.0.4",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
